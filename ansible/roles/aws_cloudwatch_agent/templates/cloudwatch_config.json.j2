{
  "agent": {
    "logs": {
      "agent_log_file": "{{ aws_cloudwatch_agent_log_file }}"
    }
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "{{ aws_cloudwatch_agent_client_log_file_path }}",
            "log_group_name": "{{ aws_cloudwatch_agent_log_group_name }}",
            "log_stream_name": "{{ aws_cloudwatch_agent_log_stream_name }}",
            "timestamp_format": "%Y-%m-%dT%H:%M:%S.%f"
          }
        ]
      }
    }
  }
}
