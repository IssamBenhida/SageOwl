{
  "agent": {
    "logs": {
      "agent_log_file": "{{ agent_log_file }}"
    }
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "{{ application_log_file_path }}",
            "log_group_name": "{{ log_group_name }}",
            "log_stream_name": "{{ log_stream_name }}",
            "timestamp_format": "%Y-%m-%dT%H:%M:%S.%f"
          }
        ]
      }
    },
    "endpoint_override": "http://localhost:4566"
  }
}
